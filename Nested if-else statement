x=int(input("enter the grade : "))
if x>=90:
    print("you are 'S' grade")
else:
    if x>=80:
        print("you are 'B' grade")
    else:
        if x>=70:
            print("you are 'C' grade")
        else:
            if x>=60:
                print("you are 'D' grade")
            else:
                if x>=50:
                    print("you are 'E' grade")
                else:
                    print("you are not eligible")
